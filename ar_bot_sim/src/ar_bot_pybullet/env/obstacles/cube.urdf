<?xml version="1.0"?>
<robot name="cube">

  <material name="red">
    <color rgba="255 0 0 1"/>
  </material>

  <material name="yellow">
    <color rgba="255 255 0 1"/>
  </material>

  <link name="cube_link">
    <contact>
      <lateral_friction value="1" />
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <mass value=".0" />
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
    </inertial>
    <visual>
        <origin rpy="0 0 0" xyz="0 0 0" />

        <geometry>
            <box size="0.1 0.1 0.1" />
        </geometry>
        <material name="red" />
    </visual>
    <collision>
        <geometry>
            <box size="0.1 0.1 0.1" />
        </geometry>
    </collision>
  </link>

</robot>